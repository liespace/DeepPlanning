{
  "Model": {
    "name": "alpha",
    "epoch": 130,
    "ini_epoch": 0,
    "verbose": 1,
    "ts_step": 2,
    "vs_step": 2,
    "batch": 1,
    "i_shape": [480, 480, 3],
    "o_shape": [30, 30, 15],
    "A": 4,
    "B": 3,
    "C": 1,
    "S": 30,
    "M": 5,
    "w": 60.0,
    "h": 60.0
  },

  "Loss": {
    "type": "self",
    "lam0": 0.1,
    "lam1": 0.5
  },

  "Optimizer": {
    "type": "adam",
    "lr": 0.001,
    "beta_1": 0.9,
    "beta_2": 0.999
  },

  "TensorBoard": {
    "enable": true,
    "write_graph": false
  },

  "LRSchedule": {
    "enable": true,
    "wp_step": 400,
    "wp_lr": 0.0005,
    "type": "cosine"
  },

  "CheckPoint": {
    "enable": true,
    "monitor": "loss",
    "only_weights": true,
    "period": 100
  },

  "EarlyStop": {
    "enable": false,
    "monitor": "loss",
    "patient": 100,
    "min_delta": 0.001,
    "verbose": 1,
    "mode": "auto"
  }
}